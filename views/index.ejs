    let allBreeds = {};
    let currentBreed = "";
    let currentSubBreed = "";

    const breedSelect = document.getElementById('breedSelect');
    const subBreedSelect = document.getElementById('subBreedSelect');
    const subBreedContainer = document.getElementById('subBreedContainer');
    const resultDiv = document.getElementById('result');
    const detailsDiv = document.getElementById('details');
    const detailsBtn = document.getElementById('detailsBtn');
    const anotherBtn = document.getElementById('anotherBtn');
    const searchBtn = document.getElementById('searchBtn');

    async function loadBreeds() {
      const res = await fetch('https://dog.ceo/api/breeds/list/all');
      const data = await res.json();
      allBreeds = data.message;
      const breeds = Object.keys(allBreeds);
      breeds.forEach(breed => {
        let option = document.createElement('option');
        option.value = breed;
        option.textContent = breed.charAt(0).toUpperCase() + breed.slice(1);
        breedSelect.appendChild(option);
      });
    }

    function loadSubBreeds() {
      const breed = breedSelect.value;
      const subBreeds = allBreeds[breed];
      subBreedSelect.innerHTML = "";

      if (subBreeds && subBreeds.length > 0) {
        subBreedContainer.style.display = "block";
        subBreeds.forEach(sb => {
          let option = document.createElement('option');
          option.value = sb;
          option.textContent = sb.charAt(0).toUpperCase() + sb.slice(1);
          subBreedSelect.appendChild(option);
        });
      } else {
        subBreedContainer.style.display = "none";
      }
    }

    async function fetchDogImage(breed, sub) {
      const url = sub
        ? `https://dog.ceo/api/breed/${breed}/${sub}/images/random`
        : `https://dog.ceo/api/breed/${breed}/images/random`;
      const res = await fetch(url);
      if (!res.ok) throw new Error("Failed API");
      const data = await res.json();
      return data.message;
    }

    searchBtn.addEventListener('click', async () => {
      const breed = breedSelect.value;
      const sub = subBreedSelect.value || "";
      if (!breed) {
        alert("Please select a breed first.");
        return;
      }
      resultDiv.innerHTML = '<div class="spinner"></div>';
      detailsBtn.style.display = 'none';
      anotherBtn.style.display = 'none';
      detailsDiv.style.display = 'none';
      detailsDiv.innerHTML = "";

      try {
        const imageUrl = await fetchDogImage(breed, sub);
        resultDiv.innerHTML = `
          <h2>${breed.charAt(0).toUpperCase() + breed.slice(1)} ${sub ? "- " + sub : ""}</h2>
          <img id="dogImage" src="${imageUrl}" alt="dog image" />
        `;
        detailsBtn.style.display = "inline-block";
        anotherBtn.style.display = "inline-block";
        currentBreed = breed;
        currentSubBreed = sub;
      } catch (err) {
        resultDiv.innerHTML = '<p style="color:red;">Failed to load image. Try again.</p>';
      }
    });

    anotherBtn.addEventListener('click', async () => {
      if (!currentBreed) return;
      const imgEl = document.getElementById('dogImage');
      if (!imgEl) return;
      imgEl.src = "";
      imgEl.alt = "Loading...";
      try {
        const newImg = await fetchDogImage(currentBreed, currentSubBreed);
        imgEl.src = newImg;
      } catch (err) {
        alert("Failed to load another image. Try again.");
      }
    });

    detailsBtn.addEventListener('click', () => {
      const info = breedInfo[currentBreed.toLowerCase()];
      if (info) {
        detailsDiv.innerHTML = `
          <strong>Origin:</strong> ${info.origin}<br>
          <strong>Size:</strong> ${info.size}<br>
          <strong>Temperament:</strong> ${info.temperament}<br>
          <strong>Life Span:</strong> ${info.life_span}<br>
          <strong>Coat:</strong> ${info.coat}<br>
          <strong>Colors:</strong> ${info.colors}<br>
          <strong>Grooming:</strong> ${info.grooming}<br>
          <strong>Exercise:</strong> ${info.exercise}<br>
          <strong>Good with Children:</strong> ${info.good_with_children}<br>
          <strong>Trainability:</strong> ${info.trainability}<br>
          <strong>Health Issues:</strong> ${info.health_issues}<br>
          <strong>Temperature Tolerance:</strong><br>
          &nbsp;&nbsp;Winter: ${info.temperature_tolerance.winter}<br>
          &nbsp;&nbsp;Spring: ${info.temperature_tolerance.spring}<br>
          &nbsp;&nbsp;Summer: ${info.temperature_tolerance.summer}<br>
          &nbsp;&nbsp;Autumn: ${info.temperature_tolerance.autumn}
        `;
      } else {
        detailsDiv.innerHTML = "<em>Information not available for this breed.</em>";
      }
      detailsDiv.style.display = "block";
    });

    window.onload = loadBreeds;
  </script>
